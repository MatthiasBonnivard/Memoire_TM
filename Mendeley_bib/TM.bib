keywords = {Virtual Reality,movement therapy,rehabilitation,serious games,stroke},
pages = {1872--1877},
title = {{Serious games for movement therapy after stroke}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=4811562},
year = {2008}
}
