@article{Prasad2002c,
abstract = {Appropriate, regular exercise is of benefit to patients with cystic fibrosis (CF). As with other segments of the population, it has been difficult to devise exercise programs to which most patients will adhere for long periods of time. In healthy children, factors that are related to positive exercise compliance include social support, perceptions of competency and self-esteem, enjoyment of activity, and availability of a variety of activities. In patients with CF, complications associated with the disease, e.g., time required for other treatment and fatigue, make compliance with recommended exercise activities more difficult. Factors likely to increase compliance in this population include explicit and continued encouragement and support from the family and healthcare team, and the introduction of behavior-changing strategies.},
author = {Prasad, S. a. and Cerny, F. J.},
doi = {10.1002/ppul.10126},
file = {:C$\backslash$:/Users/Nicolas/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Prasad, Cerny - 2002 - Factors that influence adherence to exercise and their effectiveness Application to cystic fibrosis.pdf:pdf},
isbn = {87556863 (ISSN)},
issn = {87556863},
journal = {Pediatric Pulmonology},
keywords = {Adherence to exercise,Behavior change,Chronic lung disease,Cystic fibrosis,Exercise},
number = {April},
pages = {66--72},
pmid = {12112800},
title = {{Factors that influence adherence to exercise and their effectiveness: Application to cystic fibrosis}},
volume = {34},
year = {2002}
}
