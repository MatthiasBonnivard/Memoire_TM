issn = {21910367},
journal = {International Journal on Disability and Human Development},
keywords = {Customised non-formal rehabilitation,Interaction devices,Serious gaming},
pages = {5--9},
title = {{Customising games for non-formal rehabilitation}},
volume = {10},
year = {2011}
