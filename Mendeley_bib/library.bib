journal = {Journal of cystic fibrosis : official journal of the European Cystic Fibrosis Society},
keywords = {*Cystic Fibrosis/dt [Drug Therapy],*Medication Adherence/sn [Statistics {\&} Numerical D,*Respiratory Therapy/sn [Statistics {\&} Numerical Da,Adherence,Adolescence,Adolescent,Child,Cystic fibrosis,Female,Humans,Male,Nebuliser,Nebulizers and Vaporizers,Retrospective Studies,Schools},
number = {5},
pages = {440--444},
