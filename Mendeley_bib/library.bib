journal = {Chronic illness},
keywords = {chronic illness,distance monitoring,duality of technology,health technology,health technology design,illness solved through,introduction,is chronic,or is},
pages = {85--97},
pmid = {18583444},
title = {{The duality of health technology in chronic illness: how designers envision our future.}},
