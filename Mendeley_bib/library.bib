number = {April},
pages = {1--34},
publisher = {The SROI Network and SIAA Social Impact Analysts Association},
title = {{Influencing Impact. Understanding impact measurement in social investment}},
year = {2015}
}
@book{SocialValueNetworkUK2015,
abstract = {The use of private capital to positively affect social change—that is, social investment—has grown in popularity for a variety of reasons. Education, housing, labor, and other public welfare initiatives, traditionally funded by the state in countries such as Germany and the United Kingdom, have experienced reductions in resources since 2008 (at a minimum) at the advent of the global economic crisis. The decrease in breadth and scope of public services, as well as increased barriers to the access of services, have created a space for social entrepreneurs to fill important voids as service providers in this market. These conditions have also given way to demands for innovations and services that tend to be privately held and privately funded. Therefore, as public sector funds continue to diminish, the need for and prominence of social entrepreneurship will certainly continue to grow. The success of a social enterprise hinges on a number of related factors. Consider, for instance, the entrepreneur’s ability to secure investment, the viability of the business plan, and the enterprise’s ability to adapt and respond to fluctuating market conditions. Further, because a successful enterprise must, by definition, be income-generating, gauging the extent to which the organization is able to realize its goals as a result of having sought out and received private investments has become an integral aspect of this work. As such, not only does “success” mean many things within the social investment space, but demonstrating “success” requires attention to many moving parts within an organization as well.},
author = {{Social Value Network UK}},
