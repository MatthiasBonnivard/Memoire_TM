journal = {Pediatric allergy, immunology, and pulmonology},
number = {2},
pages = {82--86},
pmid = {24963456},
title = {{Family Functioning and Treatment Adherence in Children and Adolescents with Cystic Fibrosis.}},
url = {http://europepmc.org/articles/PMC4062112/?report=abstract},
volume = {27},
year = {2014}
}
