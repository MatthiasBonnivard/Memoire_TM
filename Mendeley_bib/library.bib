doi = {10.1109/TNSRE.2012.2207462},
file = {:C$\backslash$:/Users/Nicolas/Dropbox/TM/Documents/R{\'{e}}f{\'{e}}rences/18{\_}[55]{\_}Kim{\_}KinematicDataWearableRobot.pdf:pdf},
isbn = {1558-0210},
issn = {15344320},
journal = {IEEE Transactions on Neural Systems and Rehabilitation Engineering},
keywords = {Evaluation metric,exoskeleton,stroke rehabilitation,video game},
number = {2},
pages = {153--164},
pmid = {22855233},
title = {{Kinematic data analysis for post-stroke patients following bilateral versus unilateral rehabilitation with an upper limb wearable robotic system}},
